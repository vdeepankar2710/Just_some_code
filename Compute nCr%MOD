#include<bits/stdc++.h>
using namespace std;
#define ll long long
#define MAX 100001
#define MOD 1000000007
// #define MOD 998244353
#define pb push_back
#define f first
#define s second
#define INF 2e18
#define PII pair<int,int>
#define PLL pair<ll, ll>

ll fac[10*MAX];
void precompute()
{
    fac[0] = 1;
    for(int i=1; i<10*MAX; i++)
        fac[i] = (fac[i-1]*i)%MOD;
}

ll power(ll x, ll y) //x^y
{
    ll res = 1;
    x = x%p;
    while(y>0)
    {
        if(y%2)
            res = (res*x)%MOD;
        y/=2;
        x = (x*x)%MOD;
    }
    return res;
}
ll modInv(ll n)
{
    return power(n, MOD-2);
}
ll nCr(ll n, ll r)
{
    if(r<=0)    return 1;
    return (fac[n]*(modInv(fac[r])%MOD)*(modInv(fac[n-r])%MOD))%MOD;
}
int main()
{
  return 0;
}
