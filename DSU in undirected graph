#include<bits/stdc++.h>
using namespace std;
#define ll long long
#define MAX 100001
#define MOD 1000000007
#define pb push_back
#define f first
#define s second
#define INF 2e18
#define PII pair<int,int>
#define PLL pair<ll, ll>

vector<int> parent, rnk;
void init(int n)
{
	parent.clear();
	parent.resize(n+1);
	for(int i=0; i<n+1; i++)
	parent[i] = i;
	rnk.clear();
	rnk.resize(n+1, 0);
}
int find_set(int v)
{
	if(v==parent[v])	return v;
	return parent[v] = find_set(parent[v]);
}
void union_sets(int a, int b)
{
	a = find_set(a);
	b = find_set(b);
	if(a!=b)
	{
		if(rnk[a]<rnk[b])
		swap(a, b);
		parent[b] = a;
		if(rnk[a] == rnk[b])
		rnk[a]++;
	} 
}
int main()
{
    int t; cin>>t;
    while(t--)
    {
        int n, k; cin>>n>>k;
        init(n);
        
        
    }
}
